<?xml version="1.0" encoding="UTF-8"?>
<properties>	
	<wsdlLocationUri>http://[to be defined]/2.0/</wsdlLocationUri>
	
	<server>
		<url>http://[to be defined]</url>
	</server>
	
	<reporting>
		<birt>
			<url>http://[to be defined]/birt</url>
			<url_intern>http://[to be defined]:8080/birt</url_intern>
			<drilldownurl>http://[to be defined]</drilldownurl>
		</birt>
	</reporting>
	
	<mailgun>
		<system>localhost</system>
		<ini>
			<domain>openemm.org</domain>
			<mailer>Agnitas OpenEMM ${ApplicationMajorVersion}.${ApplicationMinorVersion}</mailer>
			<metadir>/home/openemm/var/spool/META</metadir>
			<xmlback>/home/openemm/bin/xmlback</xmlback>
			<account_logfile>/home/openemm/log/account.log</account_logfile>
			<directdir>/home/openemm/var/spool/DIRECT</directdir>
		</ini>
	</mailgun>
	
	<mailaddresses>
		<sender>openemm-ws@example.com</sender>
		<sender_name>OpenEMM WS</sender_name>
		<replyto>openemm-ws@example.com</replyto>
		<replyto_name>OpenEMM WS</replyto_name>
		<bounce>openemm-ws@example.com</bounce>
		
		<frontend>openemm-ws@example.com</frontend>
		<support>openemm-ws@example.com</support>
		<error>openemm-ws@example.com</error>
		<report_archive>openemm-ws@example.com</report_archive>
	</mailaddresses>
	
	<htmlPdfConverter>${wkhtmltopdf}</htmlPdfConverter>
	<htmlImageConverter>${wkhtmltoimage}</htmlImageConverter>
</properties>
